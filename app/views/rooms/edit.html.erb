<% form_for([@property,@room]) do |f| %>
  <%= f.error_messages %>
  <fieldset id="room">
		<legend>Edit room for <%= @property.name %></legend>
   	<%= f.label :name %><%= f.text_field :name %><br />
	  <%= f.submit "Update", :class => 'button' %>
		<%= button_to('Cancel', '', :class => "button",:onclick => "window.location = \"#{property_path(@property)}\";return false") %>
  </fieldset>
<% end %>